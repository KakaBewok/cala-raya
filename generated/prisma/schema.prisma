generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model gift_infos {
  id                    Int          @id @default(autoincrement())
  invitation_id         Int?
  provider_name         String?      @db.VarChar(255)
  account_number        String?      @db.VarChar(255)
  account_holder        String?      @db.VarChar(255)
  gift_delivery_address String?
  created_at            DateTime?    @default(now()) @db.Timestamp(6)
  updated_at            DateTime?    @default(now()) @db.Timestamp(6)
  invitations           invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model guests {
  id            Int          @id(map: "guests_pkey1") @default(autoincrement())
  invitation_id Int?
  name          String?      @db.VarChar(255)
  phone_number  String?      @db.VarChar(20)
  slug          String?      @db.VarChar(255)
  is_attending  Boolean?
  total_guest   Int?
  notes         String?
  address       String?
  created_at    DateTime?    @default(now()) @db.Timestamp(6)
  updated_at    DateTime?    @default(now()) @db.Timestamp(6)
  invitations   invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "guests_invitation_id_fkey1")
}

model images {
  id            Int          @id @default(autoincrement())
  invitation_id Int?
  url           String?      @db.VarChar(255)
  caption       String?      @db.VarChar(255)
  type          String?      @db.VarChar(50)
  created_at    DateTime?    @default(now()) @db.Timestamp(6)
  updated_at    DateTime?    @default(now()) @db.Timestamp(6)
  order_number  String?      @db.VarChar
  invitations   invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model invitations {
  id                       Int          @id(map: "invitations_pkey1") @default(autoincrement())
  user_id                  Int?
  theme_id                 Int?
  music_id                 Int?
  phone_number             String?      @db.VarChar(20)
  slug                     String?      @db.VarChar(255)
  event_title              String?      @db.VarChar(255)
  host_one_name            String?      @db.VarChar(255)
  host_two_name            String?      @db.VarChar(255)
  host_one_nickname        String?      @db.VarChar(255)
  host_two_nickname        String?      @db.VarChar(255)
  host_one_additional_info String?
  host_two_additional_info String?
  host_one_social_media    String?      @db.VarChar(255)
  host_two_social_media    String?      @db.VarChar(255)
  event_date               DateTime?    @db.Date
  event_type               String?      @db.VarChar(255)
  location                 String?      @db.VarChar(255)
  location_url             String?
  message                  String?
  hashtag                  String?
  is_active                Boolean?     @default(true)
  activated_at             DateTime?    @db.Timestamp(6)
  expired_at               DateTime?    @db.Timestamp(6)
  created_at               DateTime?    @default(now()) @db.Timestamp(6)
  updated_at               DateTime?    @default(now()) @db.Timestamp(6)
  web_url                  String?      @db.VarChar
  message_template         String?
  additional_info          String?      @default("") @db.VarChar
  location_detail          String?      @db.VarChar
  gift_infos               gift_infos[]
  guests                   guests[]
  images                   images[]
  music                    music?       @relation(fields: [music_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  themes                   themes?      @relation(fields: [theme_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users                    users?       @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  rsvps                    rsvps[]
  rundowns                 rundowns[]
  stories                  stories[]
  videos                   videos[]
}

model music {
  id          Int           @id @default(autoincrement())
  title       String?       @db.VarChar(255)
  artist      String?       @db.VarChar(255)
  url         String?       @db.VarChar(255)
  created_at  DateTime?     @default(now()) @db.Timestamp(6)
  updated_at  DateTime?     @default(now()) @db.Timestamp(6)
  invitations invitations[]
}

model rsvps {
  id                Int          @id @default(autoincrement())
  invitation_id     Int?
  guest_name        String?      @db.VarChar(255)
  message           String?
  attendance_status Boolean?
  total_guest       Int?
  created_at        DateTime?    @default(now()) @db.Timestamp(6)
  updated_at        DateTime?    @default(now()) @db.Timestamp(6)
  icon_color        String?      @db.VarChar
  invitations       invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model rundowns {
  id              Int          @id @default(autoincrement())
  invitation_id   Int?
  title           String?      @db.VarChar(255)
  location        String?      @db.VarChar(255)
  location_url    String?
  date            DateTime?    @db.Date
  start_time      DateTime?    @db.Time(6)
  end_time        DateTime?    @db.Time(6)
  time_zone       String?      @db.VarChar(50)
  description     String?
  image_url       String?      @db.VarChar(255)
  order_number    Int?
  created_at      DateTime?    @default(now()) @db.Timestamp(6)
  updated_at      DateTime?    @default(now()) @db.Timestamp(6)
  location_detail String?      @db.VarChar
  invitations     invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model stories {
  id            Int          @id @default(autoincrement())
  invitation_id Int?
  title         String?      @db.VarChar(255)
  content       String?
  image_url     String?      @db.VarChar(255)
  story_date    DateTime?    @db.Date
  order_number  Int?
  created_at    DateTime?    @default(now()) @db.Timestamp(6)
  updated_at    DateTime?    @default(now()) @db.Timestamp(6)
  invitations   invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model themes {
  id            Int           @id @default(autoincrement())
  name          String?       @db.VarChar(255)
  preview_image String?       @db.VarChar(255)
  is_active     Boolean?      @default(true)
  description   String?
  created_at    DateTime?     @default(now()) @db.Timestamp(6)
  updated_at    DateTime?     @default(now()) @db.Timestamp(6)
  invitations   invitations[]
}

model users {
  id             Int           @id @default(autoincrement())
  name           String?       @db.VarChar(255)
  email          String?       @unique @db.VarChar(255)
  password       String?       @db.VarChar(255)
  remember_token String?       @db.VarChar(255)
  created_at     DateTime?     @default(now()) @db.Timestamp(6)
  updated_at     DateTime?     @default(now()) @db.Timestamp(6)
  role           String?       @db.VarChar
  invitations    invitations[]
}

model videos {
  id            Int          @id @default(autoincrement())
  invitation_id Int?
  title         String?      @db.VarChar(255)
  description   String?
  url           String?      @db.VarChar(255)
  thumbnail     String?      @db.VarChar(255)
  order_number  Int?
  created_at    DateTime?    @default(now()) @db.Timestamp(6)
  updated_at    DateTime?    @default(now()) @db.Timestamp(6)
  invitations   invitations? @relation(fields: [invitation_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
